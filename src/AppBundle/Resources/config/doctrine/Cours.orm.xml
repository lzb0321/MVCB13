<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cours" table="cours">
    <id name="idcours" type="integer" column="idcours">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nomcours" type="string" column="nomcours" length="32" nullable="true"/>
    <field name="nbcreditcours" type="integer" column="nbcreditcours" nullable="true"/>
    <field name="specialitecours" type="string" column="specialitecours" length="32" nullable="true"/>
  </entity>
</doctrine-mapping>
